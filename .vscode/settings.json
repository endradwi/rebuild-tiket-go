{
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "prettier.requireConfig": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "security.workspace.trust.untrustedFiles": "open",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "tabnine.experimentalAutoImports": true,
  "editor.maxTokenizationLineLength": 2000000,
  "editor.largeFileOptimizations": false,
  "workbench.colorTheme": "Dracula Theme",
  "powermode.enabled": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.zoomLevel": 1,
  "tailwindCSS.experimental.classRegex": [
  ["cva\\(((?:[^()]|\\([^()]\\)))\\)", "[\"']([^\"']).?[\"'`]"],
  ["cx\\(((?:[^()]|\\([^()]\\)))\\)", "(?:'|\"|)([^']*)(?:'|\"|)"]
],
"editor.tabSize": 2,
"tailwindCSS.experimental.configFile": "src/shared/styles/globals.css",
"editor.codeActionsOnSave": {
  "source.fixAll.eslint": "explicit"
},
"eslint.validate": [
  "javascript",
  "javascriptreact",
  "typescript",
  "typescriptreact"
],
"files.autoSave": "afterDelay",
"workbench.iconTheme": "material-icon-theme",
"workbench.startupEditor": "none",
"github.copilot.nextEditSuggestions.enabled": false,
"gitlens.ai.model": "vscode",
"gitlens.ai.vscode.model": "copilot:gpt-4",
"workbench.editor.empty.hint": "hidden",
"editor.minimap.enabled": false,
"editor.stickyScroll.enabled": false,
"json.schemas": [
  
],
"editor.fontSize": 14,
"eslint-service.packageManager": "pnpm",
"eslint-service.logLevel": "none",
"eslint-service.ignoreTypeScript": false,
"eslint-service.ignoreJavaScript": false,
"eslint-service.alwaysShowRuleFailuresAsWarnings": true,
"eslint-service.configFile": "",
}